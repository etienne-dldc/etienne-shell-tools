{
  "name": "etienne-shell-tools",
  "description": "custom toolforshell",
  "version": "1.0.0",
  "author": "Etienne Dldc",
  "bin": {
    "est": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^2.4.1",
    "git-url-parse": "^10.0.1",
    "ora": "^3.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/git-url-parse": "^9.0.0",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/ora": "^1.3.4",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^12",
    "ts-node": "^7",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "est",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "posttest": "tsc -p test --noEmit",
    "prepare": "rm -rf lib && tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
