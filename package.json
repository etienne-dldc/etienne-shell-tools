{
  "name": "etienne-shell-tools",
  "description": "custom toolforshell",
  "version": "1.0.0",
  "author": "Etienne Dldc",
  "bin": {
    "est": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.5.14",
    "@oclif/config": "^1.13.0",
    "@oclif/plugin-help": "^2.2.0",
    "chalk": "^2.4.2",
    "git-url-parse": "^11.1.2",
    "got": "^9.6.0",
    "inquirer": "^6.3.1",
    "ora": "^3.4.0",
    "prettier": "^1.17.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@types/git-url-parse": "^9.0.0",
    "@types/got": "^9.4.4",
    "@types/inquirer": "^6.0.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.4",
    "@types/prettier": "^1.16.4",
    "globby": "^9.2.0",
    "mocha": "^6.1.4",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "est",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "posttest": "tsc -p test --noEmit",
    "prepare": "rm -rf lib && tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
