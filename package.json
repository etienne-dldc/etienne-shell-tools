{
  "name": "etienne-shell-tools",
  "version": "1.0.0",
  "description": "custom toolforshell",
  "license": "MIT",
  "author": "Etienne Dldc",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "est": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "prepare": "rm -rf lib && tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "tsc -p test --noEmit",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.7.0",
    "@oclif/config": "^1.16.0",
    "@oclif/plugin-help": "^3.1.0",
    "chalk": "^4.1.0",
    "git-url-parse": "^11.1.2",
    "got": "^11.5.1",
    "inquirer": "^7.3.3",
    "ora": "^4.0.5",
    "prettier": "^2.0.5",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/git-url-parse": "^9.0.0",
    "@types/got": "^9.6.11",
    "@types/inquirer": "^7.3.0",
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.0.27",
    "@types/prettier": "^2.0.2",
    "globby": "^11.0.1",
    "mocha": "^8.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "est",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
